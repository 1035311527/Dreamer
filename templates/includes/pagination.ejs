<% if (pagination.prev ||pagination.next) { %>
    <div class="pagination js-pagination">
        <% if (pagination.prev) { %>
            <a href="<%= pagination.prev %>">上一页</a>
            <% } %>
                <% if (pagination.next) { %>
                    <a href="<%= pagination.next %>">下一页</a>
                    <% } %>
    </div>
    <% } %>