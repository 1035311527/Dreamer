<meta charset="utf-8" />
<meta name="description" content="<%= site.customConfig.description %> " />
<meta name="keywords" content="<%= site.customConfig.keywords %> ">
<meta name="viewport" content="width=device-width, initial-scale=1" />
<title>
    <%= siteTitle %>
</title>
<link rel="shortcut icon" href="<%= themeConfig.domain %>/favicon.ico?v=<%= site.utils.now %>">
<link rel="stylesheet" href="<%= themeConfig.domain %>/styles/main.css">
<link rel="stylesheet" href="<%= themeConfig.domain %>/media/css/prism.css">
<link rel="stylesheet" href="<%= themeConfig.domain %>/media/css/font-awesome.min.css">
<script type='text/javascript' src='<%= themeConfig.domain %>/media/js/jquery.min.js'></script>
<script type='text/javascript' src='<%= themeConfig.domain %>/media/js/plugins.js'></script>
<script type='text/javascript' src='<%= themeConfig.domain %>/media/js/script.js'></script>
<script type='text/javascript' src='<%= themeConfig.domain %>/media/js/aos.js'></script>
<script type='text/javascript' src='<%= themeConfig.domain %>/media/js/prism.js'></script>
<% if (typeof commentSetting !== 'undefined' && commentSetting.showComment) { %>
    <% if (commentSetting.commentPlatform === 'gitalk') { %>
    <link rel="stylesheet" href="https://unpkg.com/gitalk/dist/gitalk.css">
    <script src="https://unpkg.com/gitalk/dist/gitalk.min.js"></script>
    <% } %>
    <% if (commentSetting.commentPlatform === 'disqus') { %>
    <link rel="stylesheet" href="https://unpkg.com/disqusjs@1.1/dist/disqusjs.css">
    <script src="https://unpkg.com/disqusjs@1.1/dist/disqus.js"></script>
    <% } %>
    <% } %>
    <% if(site.customConfig.valineID && site.customConfig.valineKey){ %> 
        <script src='//unpkg.com/valine/dist/Valine.min.js'></script>
    <% } %> 


<% if (site.customConfig.ga) { %>
<script async src="https://www.googletagmanager.com/gtag/js?id=<%= site.customConfig.ga %>"></script>
<script>
    window.dataLayer = window.dataLayer || [];

    function gtag() {
        dataLayer.push(arguments);
    }
    gtag('js', new Date());

    gtag('config', '<%= site.customConfig.ga %>');
</script>
<% } %>
