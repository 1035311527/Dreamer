
<% if(site.customConfig.backToTop){ %>
    <div id="backToTop" style="display: block;">
        <img src="https://moguos.oss-cn-shenzhen.aliyuncs.com/boxmoe/assets/images/top/lolisister1.gif">
      </div>
<% } %>
<%- include('./copy')%>
<script type='text/javascript' src='<%= themeConfig.domain %>/media/js/particles.js'></script>
<footer id="footer" class="overlay animated from-top">
    <div class="decor-wrapper">
        <svg id="footer-decor" class="decor top" xmlns="http://www.w3.org/2000/svg" version="1.1" viewBox="0 0 100 100"
            preserveAspectRatio="none">
            <path class="large left" d="M0 0 L50 50 L0 100" fill="rgba(255,255,255, .1)"></path>
            <path class="large right" d="M100 0 L50 50 L100 100" fill="rgba(255,255,255, .1)"></path>
            <path class="medium left" d="M0 0 L50 50 L0 66.6" fill="rgba(255,255,255, .3)"></path>
            <path class="medium right" d="M100 0 L50 50 L100 66.6" fill="rgba(255,255,255, .3)"></path>
            <path class="small left" d="M0 0 L50 50 L0 33.3" fill="rgba(255,255,255, .5)"></path>
            <path class="small right" d="M100 0 L50 50 L100 33.3" fill="rgba(255,255,255, .5)"></path>
            <path d="M0 0 L50 50 L100 0 L0 0" fill="rgba(255,255,255, 1)"></path>
            <path d="M48 48 L50 51 L52 48 L48 48" fill="rgba(255,255,255, 1)"></path>
        </svg>
    </div>
    <div class="socialize" data-aos="zoom-in">
        <% if(site.customConfig.github){ %>
            <a title="github" class="socialicon" href="<%= site.customConfig.github %> "><i class="fa fa-github" aria-hidden="true"></i></a>
        <% } %>  
        <% if(site.customConfig.twitter){ %>
            <a title="twitter" class="socialicon" href="<%= site.customConfig.twitter %> "><i class="fa fa-twitter" aria-hidden="true"></i></a>
        <% } %>  
        <% if(site.customConfig.weibo){ %>
            <a title="weibo" class="socialicon" href="<%= site.customConfig.weibo %> "><i class="fa fa-weibo" aria-hidden="true"></i></a>
        <% } %>  
        <% if(site.customConfig.facebook){ %>
            <a title="facebook" class="socialicon" href="<%= site.customConfig.facebook %> "><i class="fa fa-facebook" aria-hidden="true"></i></a>
        <% } %>  
        <% if(site.customConfig.wechat){ %>
            <a title="wechat" class="socialicon" href="<%= site.customConfig.wechat %> "><i class="fa fa-wechat" aria-hidden="true"></i></a>
            <% if(site.customConfig.wechatImg){ %>
                <div class="wechatimg"><img src="<%= site.customConfig.wechatImg %> "></div>
            <% }%> 
        <% } %>  
        <% if(site.customConfig.qq){ %>
            <a title="qq" class="socialicon" href="<%= site.customConfig.qq %> "><i class="fa fa-qq" aria-hidden="true"></i></a>
        <% } %>  

    </div>
    <div class="cr">
        Copyright&copy;
        <%- themeConfig.footerInfo %> |
        <a class="rss" href="<%= themeConfig.domain %>/atom.xml" target="_blank">RSS</a>
    </div>
</footer>